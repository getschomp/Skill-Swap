<row>
  <div class="small-2 medium-4 large-4 columns">
    <h3>Profile for:</h3> <p><%= @user.username %></p>
      <ul>
        <h5>About me:</h5>
        <p><%= @user.about_me %></p>
      </ul>
      <ul>
        <h5>Gender:</h5>
        <p><%= @user.gender %></p>
      </ul>

    </ul>
  </div>
  <div class="small-2 medium-4 large-4 columns">
    <h3>Skills <%= @user.username %> wants to know</h3>
    <ul>
      <h5></h5>
      <p><% @user.wanted_skills.each do |s| %></p>
      <%= s.skill.name %>
      <% end %>
    </ul>
  </div>
  <div class="small-2 medium-4 large-4 columns">
    <h3>Skills <%= @user.username %> knows</h3>
    <ul>
      <h5></h5>
      <p><% @user.had_skills.each do |s| %></p>
        <%= s.skill.name %>
      <% end %>
    </ul>
  </div>

</row>
